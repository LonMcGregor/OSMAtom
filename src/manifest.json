{
  "manifest_version": 2,
  "name": "OSM History Feed",
  "version": "1.0.0",

  "description": "Get osm history feeds",

  "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
  },

  "permissions": [
    "https://www.openstreetmap.org/*",
    "downloads",
    "storage"
  ],

  "background": {
    "scripts": ["bg.js"],
    "persistent": false
  }
}
